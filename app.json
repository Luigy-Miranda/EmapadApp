{
  "expo": {
    "name": "Emapad",
    "slug": "Emapad",
    "version": "5.0.5",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "notification": {
      "icon": "./assets/adaptive-icon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.emapad.app"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "backgroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "USER_FACING_NOTIFICACIONS"
      ],
      "package": "com.emapad.app",
      "versionCode": 9
    },
    "web": {
      "favicon": "./assets/adaptive-icon.png"
    }
  }
}
